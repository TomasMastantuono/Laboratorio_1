   "source": [
    "def gauss(x):\n",
    "    media = np.mean(x)\n",
    "    producto_gauss = 1 / ((np.sqrt(2*np.pi)) * np.std(x))\n",
    "    exp_gauss = -(x - media)**2 / (2*(np.std(x))**2)\n",
    "    return producto_gauss*np.exp(exp_gauss)"
   ]
